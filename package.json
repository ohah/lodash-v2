{
  "name": "lodash-v2",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "test"
  ],
  "scripts": {
    "build": "bun run --filter='*' build",
    "test": "bun test",
    "test:core": "bun test packages/core",
    "test:equivalence": "bun run --filter=@lodash-v2/test test",
    "lint": "oxlint .",
    "format": "oxfmt .",
    "format:check": "oxfmt . --check",
    "benchmark": "bun run --filter=@lodash-v2/test run",
    "benchmark:compare": "bun run --filter=@lodash-v2/test compare",
    "generate-feedback": "bun run --filter=@lodash-v2/test generate-feedback"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "es-toolkit": "1.44",
    "lodash": "^4.17.23",
    "oxfmt": "latest",
    "oxlint": "latest",
    "typescript": "^5"
  }
}
